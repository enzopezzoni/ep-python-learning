    "import csv\n",
    "\n",
    "total_amount = 0\n",
    "\n",
    "with open('uber_eats.csv') as data_file:\n",
    "    data = csv.reader(data_file)\n",
    "\n",
    "    #find the total amount spent on uber eats\n",
    "    for row in data:\n",
    "        if row[2] != 'price':\n",
    "            total_amount += float(row[2])\n",
    "\n",
    "print(total_amount)"
   ]
  }
